@model OtelRezervationMvc.Models.TblYeniKayit
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html lang="en">
<head>
    <title>Otel Kayıt Formu</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Account Register Form Responsive Widget,Login form widgets, Sign up Web forms , Login signup Responsive web form,Flat Pricing table,Flat Drop downs,Registration Forms,News letter Forms,Elements" />
    <script type="application/x-javascript">
         addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
        function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Meta tag Keywords -->
    <!-- css files -->
    <link rel="stylesheet" href="~/hotels-starter/Register/css/style.css" type="text/css" media="all" /> <!-- Style-CSS -->
    <link rel="stylesheet" href="~/hotels-starter/Register/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->
    <link href="//fonts.googleapis.com/css?family=Noto+Sans:400,400i,700,700i&amp;subset=cyrillic,cyrillic-ext,devanagari,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet">

    <!-- //css files -->
</head>
<body>
    <!-- main -->
    <div class="w3ls-header">
        <h1>Otel Misafir Kayıt Formu</h1>
        <div class="header-main">
            <h2>Kayıt Formu</h2>
            <div class="header-bottom">
                <div class="header-right w3agile">
                    <div class="header-left-bottom agileinfo">
                        <form method="post">
                            <div class="icon1">
                                <input type="text" placeholder="Adınız Soyadınız" name="AdSoyad" required="" />
                            </div>
                            <div class="icon1">
                                <input type="email" placeholder="Email Adresiniz" name="Mail" required="" />
                            </div>
                            <div class="icon1">
                                <input type="tel" placeholder="Telefon Numaranız" name="Telefon" required="" />
                            </div>
                            <div class="icon1">
                                <input type="password" placeholder="Şifreniz" name="Sifre" required="" />
                            </div>
                            <div class="login-check">
                                <label class="checkbox"><input type="checkbox" name="checkbox" checked=""><i> </i>Kullanıcı Şartlarını<a href="#">Okudum&amp; Kabul Ediyorum</a></label>
                            </div>
                            <div class="bottom">
                                <input type="submit" value="Kayıt Ol!" />
                            </div>
                            <div class="social w3layouts">
                                <div class="clear"></div>
                                <p>Zaten Üyeliğiniz Varmı?<a href="/Login/Index">Giriş Yap</a></p>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--header end here-->
    <!-- copyright start here -->
    <div class="copyright">
        <p>© 2023 CakirSoft. All rights reserved | Design by  <a href="http://www.softcakir.com/" target="_blank">CakirSoft</a></p>
    </div>
    <!--copyright end here-->
</body>
</html>
